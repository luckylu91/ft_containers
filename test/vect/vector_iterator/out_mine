vector::iterator

*it = 9
++it = 0, now *it = 0
it++ = 0, now *it = 0
--it = 0, now *it = 0
it-- = 0, now *it = 9
it[0] = 9, it[2] = 0
it += 2, now *it = 0
it -= 1, now *it = 0
*(it - 1) = 9
*(it + 1) = 0
*(1 + it) = 0
*(it - (it + 2)) = -2
*(it - (it - 1)) = 1
it == it : true
it == (it + 1) : false
it != (it + 1) : true
it != it : false
it < it : false
it < (it + 1) : true
it > it : false
it > (it - 1) : true
it <= it : true
it <= (it + 1) : true
it >= it : true
it >= (it - 1) : true

